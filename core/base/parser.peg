
CLASS_REF = _ "'" CLASS_TYPE "\\" IDENTIFIER "'"

CLASS_TYPE = _ ("value" / "entity" / "command" / "event" / "projection" / "invariant" / "query")

CLASS_IMPLIED_REF = _ "'" IDENTIFIER "'"

IDENTIFIER = [a-zA-Z]+ [a-zA-Z0-9_-]*

_ = ( WHITESPACE / EOL )*

SEMI = _ ";" _

WHITESPACE = [ \t\r]
EOL = '\n'
EOF = !.
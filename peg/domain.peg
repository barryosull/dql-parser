{
    package peg

    import (
        "fmt"
        "parser/domain"
    )
}

CMD = (CREATE / FOR)

CREATE = _ "create"i _ "domain"i _ name:NAME _ {
    return domain.Create{name};
}

FOR = _ "for"i _ "domain"i _ name:Name _ {
    return domain.For{name};
}

NAME = [a-zA-Z\-]

_ ← ( Whitespace / EOL )*

Whitespace ← [ \t\r]
EOL ← '\n'
EOF ← !.

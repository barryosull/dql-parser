{
    package peg

    import (
        "fmt"
        "parser/context"
    )
}

CMD = (CREATE / IN)

CREATE = _ "create"i _ "context"i _ name:NAME _ {
    return context.Create{name};
}

IN = _ "in"i _ "context"i _ name:Name _ {
    return context.In{name};
}

NAME = [a-zA-Z\-]

_ ← ( Whitespace / EOL )*

Whitespace ← [ \t\r]
EOL ← '\n'
EOF ← !.
